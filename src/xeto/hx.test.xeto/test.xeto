// Equip with *points*
EqA: Equip {
  points: {
    a: ZoneCo2Sensor
    b: ZoneCo2Sensor { foo }
  }
}

EqB: Equip {
  points: {
    ph.points::DischargeAirTempSensor
  }
}

EqAX: EqA {
  points: {
    c: DischargeAirTempSensor
  }
}

NestedEq: Equip {
  childEquips: {
    EqA
    EqB
  }
  points: {
    OutsideAirTempSensor
  }
}

