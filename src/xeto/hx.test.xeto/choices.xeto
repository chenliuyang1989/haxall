Color: Choice

Red: Color { red }
LightRed: Red { light }
DarkRed: Red { dark }

Blue: Color { blue }
LightBlue: Blue { dark }
DarkBlue: Blue { dark}

CarA: {
  color: Color
}

CarB: {
  color: Color?
}

CarC: {
  color: Color <multiChoice>
}

CarD: {
  color: Color? <multiChoice>
}

CarE: CarD {
  color: Color <foo>  // inherits from CarD.color
}

